{
  "name": "hylianshield/vendor-locator-bundle",
  "description": "This bundle allows to locate vendor packages inside a Symfony project.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Jan-Marten de Boer",
      "email": "johmanx@hylianshield.org",
      "role": "developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/framework-bundle": "^3.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.9"
  },
  "autoload": {
    "psr-4": {
      "HylianShield\\VendorLocatorBundle\\": "src"
    }
  },
  "archive": {
    "exclude": [
      ".gitignore"
    ]
  }
}
